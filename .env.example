# Database connection URL for Prisma
# Get this by running: npx prisma dev --detach
# Then press 'h' to view the HTTP/ORM connection URL
DATABASE_URL="prisma+postgres://localhost:PORT/"

# AWS S3 Configuration for CV uploads
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
S3_BUCKET_NAME=your-bucket-name
